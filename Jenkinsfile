Jenkinsfile (Declarative Pipeline)
pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..rx'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..rx'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying..rx'
            }
        }
    }
}
